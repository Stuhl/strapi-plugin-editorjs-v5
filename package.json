    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.3.0",
    "@strapi/strapi": "^5.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.1",
    "styled-components": "^6.1.14"
  },
  "strapi": {
    "kind": "plugin",
    "name": "strapi-plugin-editorjs",
    "displayName": "Editor JS",
    "description": "Plugin for the popular WYSIWYG editor \"editor.js\""
  },
